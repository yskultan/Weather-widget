    "eslint-plugin-vue": "^7.2.0",
    "eslint-plugin-vue-scoped-css": "^1.0.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
